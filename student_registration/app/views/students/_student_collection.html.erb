<%unless students.blank?%>
<table class="styled-table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Age</th>
            <th>Gender</th>
            <th>Phone Number</th>
            <th>Edit student</th>
            <th>Destroy</th>
        </tr>
    </thead>
    <tbody>
    	<%students.each do|student|%>
        <tr>
            <td><%=student.first_name%></td>
            <td><%=student.last_name%></td>
            <td><%=student.email%></td>
            <td><%=student.age%></td>
            <td><%=student.gender%></td>
            <td><%=student.phone_number%></td>
            <td><%= link_to 'Edit', edit_student_url(student.id) %></td>
            <td> <%= link_to 'Destroy', student_path(student),method: :delete %> </td>
        </tr>
        <%end%>
    </tbody>
</table>
<%else%>
  NO DATA
<%end%>
